# SYN FLOOD RULE
alert tcp any any -> $HOME_NET any (flags:S; msg:"POSSIBLE SYN FLOOD DOS ATTACK"; detection_filter:  track by_src, seconds 10; priorty:5; sid: 20001; rev:4;)

# PORT SCAN
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"Port Scanning Detected"; flow:to_server,established; threshold:type limit,track by_src,count 5,seconds 10; sid:1000001;)

#PHISHING LINK
alert udp any any -> $HOME_NET any (msg:"User Clicked on Phishing Link"; content:"|68 11 7c 37|"; sid:10000012;)
